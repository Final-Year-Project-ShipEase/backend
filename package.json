{
  "name": "shipease-backend",
  "version": "1.0.0",
  "description": "Database Migration Files",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon app.js",
    "pretty": "prettier --write \"**/*.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node app.js",
    "db:create": "sequelize db:create --charset utf8",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:drop": "sequelize db:migrate:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gapi": "^0.0.3",
    "gapi-script": "^1.2.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "lint": "^0.8.19",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "prettier": "^3.0.1",
    "react-google-login": "^5.2.2",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.46.0",
    "eslint-plugin-node": "^11.1.0"
  }
}
